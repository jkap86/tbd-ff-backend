{
  "name": "tbd-ff-backend",
  "version": "1.0.0",
  "description": "Fantasy Football App Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc && node -e \"require('fs').cpSync('src/migrations', 'dist/migrations', {recursive: true})\"",
    "start": "node dist/index.js",
    "migrate": "node dist/scripts/runMigrations.js",
    "migrate:dev": "node -r ts-node/register src/scripts/runMigrations.ts"
  },
  "keywords": [
    "fantasy",
    "football",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1",
    "ts-node": "^10.9.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "@types/socket.io": "^3.0.0"
  },
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  }
}
